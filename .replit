modules = ["python-3.11", "python3"]
[agent]
expertMode = true
integrations = ["python_gemini:1.0.0", "spotify:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["SDL2", "SDL2_image", "SDL2_mixer", "SDL2_ttf", "arrow-cpp", "borgbackup", "c-ares", "cacert", "cairo", "cargo", "chromedriver", "chromium", "cmake", "coreutils", "espeak-ng", "ffmpeg-full", "fontconfig", "freetype", "geckodriver", "ghostscript", "glibcLocales", "gmp", "gobject-introspection", "grpc", "gtk3", "iana-etc", "lcms2", "libGL", "libGLU", "libcxx", "liberfa", "libffi", "libiconv", "libimagequant", "libjpeg", "libpng", "libtiff", "libwebp", "libxcrypt", "openjpeg", "openssl", "pkg-config", "portmidi", "python312Full", "qhull", "rustc", "scrot", "sqlite", "tcl", "tigervnc", "tk", "util-linux", "x11vnc", "xcodebuild", "xorg.libX11", "xorg.libXext", "xorg.xinit", "xorg.xorgserver", "xsimd", "xvfb-run", "zbar", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "VATSAL AI CLI"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "VATSAL GUI (VNC)"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "VATSAL GUI"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "VATSAL Web GUI"

[[workflows.workflow]]
name = "VATSAL AI CLI"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python launchers/launch_cli.py"

[[workflows.workflow]]
name = "VATSAL GUI (VNC)"
author = "agent"

[workflows.workflow.metadata]
outputType = "vnc"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "xvfb-run -a python launchers/launch_gui.py"

[[workflows.workflow]]
name = "VATSAL GUI"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python launchers/launch_gui.py"

[workflows.workflow.metadata]
outputType = "vnc"

[[workflows.workflow]]
name = "VATSAL Web GUI"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python web_gui/server.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

